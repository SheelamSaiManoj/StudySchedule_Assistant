<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Subject_Name</th>
            <th scope="col">From</th>
            <th scope="col">To</th>
            <th scope="col">Resources</th>
            <th scope="col">Percentage of Work done</th>
          </tr>
        </thead>
    
    
        <%    tableList.forEach(function(tables) { %>

            <% if (tables.day===todayDayName){ %>
                 
                 <% tables.subjects.forEach(function(item){ %>
                    <tbody>
                        <tr>
                          <td><%=  item.subjectName  %></td>
                        </tr>
                      </tbody>
                 <%})%>
         <%   } %>
     
         <%  })  %>

    </table>
    
    <form action="/options" method="post">
        <button type="submit" class="btn btn-primary" name="" value="">go to options</button>
    </form>
</body>
</html>